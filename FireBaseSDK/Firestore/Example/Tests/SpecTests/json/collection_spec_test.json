{
  "Events are raised after watch ack": {
    "describeName": "Collections:",
    "itName": "Events are raised after watch ack",
    "tags": [],
    "config": {
      "useGarbageCollection": true
    },
    "steps": [
      {
        "userListen": [
          2,
          {
            "path": "collection",
            "filters": [],
            "orderBys": []
          }
        ],
        "stateExpect": {
          "activeTargets": {
            "2": {
              "query": {
                "path": "collection",
                "filters": [],
                "orderBys": []
              },
              "resumeToken": ""
            }
          }
        }
      },
      {
        "watchAck": [
          2
        ]
      },
      {
        "watchEntity": {
          "docs": [
            [
              "collection/key",
              1000,
              {
                "foo": "bar"
              }
            ]
          ],
          "targets": [
            2
          ]
        }
      },
      {
        "watchCurrent": [
          [
            2
          ],
          "resume-token-1001"
        ],
        "watchSnapshot": 1001,
        "expect": [
          {
            "query": {
              "path": "collection",
              "filters": [],
              "orderBys": []
            },
            "added": [
              [
                "collection/key",
                1000,
                {
                  "foo": "bar"
                }
              ]
            ],
            "errorCode": 0,
            "fromCache": false,
            "hasPendingWrites": false
          }
        ]
      }
    ]
  },

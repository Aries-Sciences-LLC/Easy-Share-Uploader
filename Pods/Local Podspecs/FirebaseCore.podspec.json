{
  "name": "FirebaseCore",
  "version": "4.0.13",
  "summary": "Firebase Core for iOS",
  "description": "Firebase Core includes FIRApp and FIROptions which provide central configuration for other Firebase services.",
  "homepage": "https://firebase.google.com",
  "license": {
    "type": "Apache",
    "file": "LICENSE"
  },
  "authors": "Google, Inc.",
  "source": {
    "git": "https://github.com/firebase/firebase-ios-sdk.git",
    "tag": "4.0.13"
  },
  "social_media_url": "https://twitter.com/Firebase",
  "platforms": {
    "ios": "7.0",
    "osx": "10.10",
    "tvos": "10.0"
  },
  "cocoapods_version": ">= 1.4.0.beta.2",
  "static_framework": true,
  "prefix_header_file": false,
  "source_files": "Firebase/Core/**/*.[mh]",
  "public_header_files": [
    "Firebase/Core/Public/*.h",
    "Firebase/Core/Private/*.h"
  ],
  "private_header_files": "Firebase/Core/Private/*.h",
  "frameworks": [
    "Foundation",
    "SystemConfiguration"
  ],
  "dependencies": {
    "GoogleToolboxForMac/NSData+zlib": [
      "~> 2.1"
    ]
  },
  "pod_target_xcconfig": {
    "OTHER_CFLAGS": "-fno-autolink -DFIRCore_VERSION=4.0.13 -DFirebase_VERSION=4.8.0"
  }
}
